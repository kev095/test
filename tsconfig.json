{
  "include": ["./src"],
  "exclude": ["/node_modules"],
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./.aws-sam",
    "target": "ES2020",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": true,
    "sourceMap": false,
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution":"node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
